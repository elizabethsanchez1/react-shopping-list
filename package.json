{
  "name": "firebaseworkouttracker",
  "version": "5.3.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --verbose",
    "rename": "node ./bin/rename.js",
    "start": "react-native start",
    "bundle": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "clean-modules": "rm -rf node_modules && npm install ",
    "clean-metro-bundler": "watchman watch-del-all && rm -rf $TMPDIR/react-* &&  rm -rf $TMPDIR/haste-* && rm -rf $TMPDIR/metro-* && rm -rf /tmp/metro-bundler-cache-*",
    "clean-start": "rm -rf node_modules && npm install && cd ios/ && rm -rf Podfile.lock && rm -rf Pods && pod install",
    "clean-pods": "cd ios/ && rm -rf Podfile.lock && rm -rf Pods && pod install",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "storybook": "storybook start"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-native": "0.59.3",
    "react-native-calendars": "^1.22.0",
    "react-native-elements": "0.18.4",
    "react-native-firebase": "^5.3.1",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-material-dropdown": "^0.11.1",
    "react-native-platform-stylesheet": "0.0.6",
    "react-native-progress-circle": "^2.0.1",
    "react-native-sortable-list": "0.0.22",
    "react-native-tab-view": "^1.3.1",
    "react-native-vector-icons": "^6.4.2",
    "react-native-walkthrough-tooltip": "^0.4.1",
    "react-navigation": "^2.18.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "babel-jest": "^24.7.1",
    "fs-extra": "^7.0.1",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "react-test-renderer": "16.8.3",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "replace-in-file": "^3.4.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
